curve(dnorm(x)-0.005, lwd = 1.5, add = TRUE)


myplot(dnorm(x) ~ x, let = "b)")
curve(dnorm(x, sd = 1.5), lwd = 1.5, add = TRUE)

myplot(dnorm(x) ~ x, let = "c)")
curve(dnorm(x, mean = 1.2), lwd = 1.5, add = TRUE)

myplot(dnorm(x, mean = 1.2) ~ x, let = "d)")
curve(dnorm(x, sd = 1.5), lwd = 1.5, add = TRUE)

