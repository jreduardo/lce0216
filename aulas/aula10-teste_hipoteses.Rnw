\begin{frame}{Conceitos iniciais}

<<echo=FALSE, fig.width=7, fig.height=5>>=

myplot <- function(formula, let = "", ...) {
    plot(formula,
         type = "l",
         lwd = 1.5,
         xaxt = "n",
         yaxt = "n",
         bty = "n")
    axis(1, labels = FALSE, tick = FALSE)
    ps <- par("usr")
    arrows(ps[1], ps[3], ps[2], ps[3], code = 2,
           length = 0.1, xpd = TRUE)
    text(x = -4, y = extendrange(ps[3:4], f = -0.1)[2],
         labels = let, cex = 1.2)
}

