%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de posição (ou tendência) central}

  Medidas de posição (ou tendência) indicam posições de interesse sobre
  a distribuição dos dados.
  \vspace{0.5cm}

  As principais medidas de posição central são a \textbf{ média},
  a \textbf{mediana} e a \textbf{moda}.
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}

  \textbf{Média aritmética} \\

  Se temos os valores $x_1,x_2,\cdots,x_n$, a média aritmética é dada por
  \begin{equation*}
    \bar{x} = \frac{1}{n} \sum_{i=1}^{n} x_i =
    \frac{x_1 + x_2 + \cdots + x_n}{n}.
  \end{equation*}
  \vspace{0.5cm}\pause

  \textbf{Exemplo}: Considere os valores 2, 7, 5, 4, 9, 11. Então
  \begin{equation*}
    \bar{x} = \frac{2+7+5+4+9+11}{6} = 6,33.
  \end{equation*}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}
  {\bf Exemplo:}\\[0.2cm]

  Com o objetivo de avaliar a produção anual de resina de árvores de
  {\it Pinus elliottii}, em kg, foram observadas dez árvores, cujos
  correspondentes valores são apresentados a seguir:

  \begin{center}
    \begin{tabular}{ccccc} \hline
      1,9 &  2,1 &  3,4 & 2,3 & 2,3 \\
      5,5 & 2,6 & 1,5 & 1,8 & 2,0\\ \hline
    \end{tabular}
  \end{center}

Calcule a média da produção anual de resina.
\vspace{-0.2cm}\pause

\begin{eqnarray*}
  \bar{x} &=& \frac{1,9 + 2,1 + \ldots + 2,0}{10} \\
          &=& \frac{25,4}{10} \\
          &=& 2,54 \text{kg}.\\
\end{eqnarray*}

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Média Aritmética Ponderada}

  A média ponderada dos números $x_1, x_2, \ldots, x_n$, com pesos
  $p_1, p_2, \ldots, p_n$, representada por $\bar{x}_p$, é definida por

  $$
  \bar{x}_p = \frac{p_1x_1 + p_2x_2 + \ldots + p_nx_n}{p_1 +
    p_2 + \ldots + p_n} = \frac{\sum_{i=1}^n p_ix_i}{\sum_{i=1}^n p_i}.
  $$

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Média Aritmética Ponderada}

  {\bf Exemplo:} Precipitação média em uma bacia hidrográfica.

  \begin{multicols}{2}

    \begin{center}
        \small
      \begin{tabular}{cc} \hline
        Precipitação & Área do polígono  \\
        % ({\it mm}) & (km$^2$) \\
        $X_i$ ({\it mm}) & $p_i$ (km$^2$) \\ \hline
        90 & 6 \\
        110 & 7 \\
        120 & 5\\
        100 & 12 \\
        112 & 11 \\
        95 & 8 \\
        108 & 1,5 \\ \hline
        Total & 50,5 \\ \hline
      \end{tabular}
    \end{center}

    \begin{figure}[H]
      \includegraphics[width=5.5cm]{images/baciahidro}
    \end{figure}
  \end{multicols}
    \pause

  $$
  \bar{x}_p = \frac{90 \times 6 + 110 \times 7 + \ldots +
    108 \times 1,5}{50,5} = 104,24.
  $$
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}

  Se os dados estão organizados em \textbf{tabelas de frequências}, caso
  das variáveis quantitativas discretas, a média aritmética pode ser
  calculada por:

  \begin{equation*}
    \bar{x} = \frac{1}{n}\sum_{i=1}^{n} x_i f_i.
  \end{equation*}

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}
  \small

  \textbf{Exemplo:} Em um estudo realizado para avaliar o número
  de brotos deixados em cepas de {\it Eucalyptus grandis} após o
  primeiro corte, foram utilizadas 40 plantas.

  \vspace{-0.2cm}
  \begin{table}
    \small
    \caption{Cálculo auxiliar da média de dados em tabela de
      frequências.}
    \begin{tabular}{c c c}
      \hline
      $x_i$ & $f_i$ &	$x_i f_i$ \\
      \hline
      0 & 7  &  0 \\
      1 & 11 & 11 \\
      2 & 14 & 28 \\
      3 & 8  & 24 \\
      \hline
      Total&	40 &  	63 \\
      \hline
    \end{tabular}
  \end{table}

  Média: $\bar{x} = \frac{63}{40}=1,575$.\\

  A média será 1,575 brotos/planta.
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}

  Se a variável é quantitativa contínua e está agrupada em intervalos de
  classe, a média aritmética é dada por:

  \begin{equation*}
    \bar{x} = \frac{1}{n} \sum_{i=1}^{n} x^{*}_{i}f_i,
  \end{equation*}

  em que $x^{*}_{i}$ é o ponto médio do intervalo de classe.

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}
  \small

  \textbf{Exemplo:} Vamos retomar ao exemplo do diâmetro das árvores em
  uma floresta.
  \vspace{0.2cm}

  Seja $x^{*}_i$: ponto médio do intervalo de classe e $f_i$: a
  frequência do intervalo de classe.
  \vspace{-0.3cm}

  \begin{table}
    \caption{\footnotesize Tabela auxiliar para o cálculo da média de
      dados em tabela de frequências com intervalos de classe.}
    \begin{tabular}{l c c c }
      \hline
      Classes &  $x^{*}_i$ & $f_i$ & $x^{*}_i f_i$\\
      \hline
      $10,2$$\vdash$$22,0$ &  16,1 & 22 & 354,20\\
      $22,0$$\vdash$$33,8$ &  27,9 & 6  &  167,40\\
      $33,8$$\vdash$$45,6$ &  39,7 & 2  & 79,40\\
      $45,6$$\vdash$$57,4$ &  51,5 & 5  & 257,50\\
      $57,4$$\vdash$$69,2$ &  63,3 & 2  & 126,60\\
      $69,2$$\vdash$$81,0$ &  75,1 & 2  & 150,20\\
      $81,0$$\vdash$$92,8$ &  86,9 & 1  & 86,90\\
      \hline
      Total &   &  40   &   1222,20 \\
      \hline
      Média &         \multicolumn{3}{c}{$1222,20/40=30,56$}\\
      \hline
    \end{tabular}
  \end{table}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}
  \small

  \textbf{Observação:} A média é sensível a observações discrepantes,
  isto é, se existirem valores fora do intervalo de maior concentração
  dos dados, esses valores influenciam fortemente a média. Neste caso, a
  média pode não ser um bom representante da tendência central dos
  dados.

  \vspace{0.5cm}

  Uma outra medida de tendência central, que é pouco influenciada por
  observações discrepantes, é a \textbf{mediana}.

\end{frame}

% -------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Média}

  \textbf{Mediana}  \\
  Se temos os valores $x_1,x_2,\cdots,x_n$, a mediana é calculada da
  forma:

  \begin{enumerate}[i]
  \item Ordene os dados em ordem crescente, ou seja,
    $x_{(1)}, x_{(2)}, \cdots, x_{(n)}$ e

  \item Calcule
    \begin{equation*}
      \text{Md} =
        \begin{dcases}
          \frac{x_{\left(\frac{n}{2}\right)} +
                x_{\left(\frac{n}{2}+1\right)}}{2}, &
            \text{se}\, n\, \text{é par}; \\
          x_{\left(\frac{n+1}{2}\right)}, &
            \text{se}\, n\, \text{é ímpar}.
        \end{dcases}
    \end{equation*}
  \end{enumerate}

  \textbf{Observação:} Note como a mediana é pouco afetada por valores
  extremos ou discrepantes.

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Mediana}

  Considere os valores 7, 9, 2, 5, 4.
  \vspace{1cm}
  \pause

  Ordenando os valores têm-se:

  $$x_{(1)}=2, x_{(2)}=4, x_{(3)}=5, x_{(4)}=7, x_{(5)}=9.$$
  \vspace{0.4cm}
  \pause

  Como $n=5$ (ímpar), a mediana será dada por
  \pause

  $$
  \text{Md}= x_{\left(\frac{n+1}{2}\right)} =
  x_{\left(\frac{5+1}{2}\right)} = x_{(3)} = 5.
  $$

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Mediana}

  {\bf Exemplo}: Para os valores observados de produção anual de resina
  de árvores de {\it Pinus elliottii}, tem-se o seguinte rol:

  \begin{center}
    \small
    \begin{tabular}{cccccccccc} \hline
      1,5 & 1,8 & 1,9 & 2,0 & 2,1 & 2,3 & 2,3 & 2,6 & 3,4 & 5,5 \\
      \hline
    \end{tabular}
  \end{center}

  \vspace{0.5cm}
  Como $n$ é par,
  \begin{center}
    \begin{tabular}{cccccccccc}
      1,5 & 1,8 & 1,9 & 2,0 & \color{teal}{\bf 2,1} &
      \color{teal}{\bf 2,3} & 2,3 & 2,6 & 3,4 & 5,5 \\
    \end{tabular}
  \end{center}
  \pause

  \begin{itemize}
  \item[]<2->
    \vspace{-0.1cm}
    \begin{eqnarray*}
      \text{Md}&=& \frac{x_{(5)} + x_{(6)}}{2} =
                   \frac{2,1 + 2,3}{2} = \frac{4,4}{2} \\
               &=&  2,2\,\text{kg}.\\
    \end{eqnarray*}
  \end{itemize}

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Mediana}
  \small

  Para dados discretos dispostos em tabela de frequências,
  a classe mediana é obtida fazendo o cálculo da \textbf{frequência
    acumulada}, que é a soma (ou valor total) de todas as frequências
  até o ponto desejado, e verificando o local no qual a
  se encontra o rol mediano.\\[0.5cm]
  \pause

  {\bf Exemplo:} Número de brotos deixados em cepas de {\it Eucalyptus
    grandis} após o primeiro corte.

  \vspace{-0.5cm}
  \begin{center}
    \begin{table}
      \caption{\footnotesize Distribuição de frequências para a variável
        número de brotos deixados em cepas de {\it Eucalyptus grandis}
        após o primeiro corte.}
      \begin{tabular}{ccc} \hline
        Número de & \\
        brotos  &  $f_i$ & $F_i$  \\ \hline
        0 & 7 &   7 \\
        1 & 11 & 18 \\
        \rowcolor{teal!10}
        \color{teal}\bf 2 & 14 & \color{teal}\bf 32 \\
        3 &  8&  40 \\ \hline
        Total  & 40 & \\ \hline
      \end{tabular}
    \end{table}
  \end{center}
  \pause

  $$\text{Md} = 2$$

\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medida de posição central - Mediana}

  Se os dados são de uma variável contínua e estiverem agrupados em
  classes, a mediana é dada por:

  \begin{equation*}
    \text{Md} = LI_{Md} + \frac{\frac{n}{2} -
      F_{\frac{n}{2}-1}}{f_{\frac{n}{2}}} h,
  \end{equation*}
  em que:

  \begin{itemize}
  \item $LI_{Md}$ é limite inferior da classe mediana,\\
  \item $n$ é o tamanho da amostra,\\
  \item $F_{\frac{n}{2}-1}$ é frequência acumulada anterior à classe
    mediana,\\
  \item $f_{\frac{n}{2}}$ é a frequência da classe mediana,\\
  \item $h$ é a amplitude do intervalo.
  \end{itemize}
\end{frame}


\begin{frame}
  \frametitle{Medida de posição central - Mediana}

  \begin{equation*}
    \text{Md} = LI_{Md} + \frac{\frac{n}{2} -
      F_{\frac{n}{2}-1}}{f_{\frac{n}{2}}} h,
  \end{equation*}
  \vspace{-1cm}

<<echo=FALSE, out.width="1\\hsize", fig.height=5, fig.width=7>>=

li <- seq(70, 100, 10)
ls <- seq(80, 110, 10)
fa <- c(30, 40, 20, 10)

ht <- list(breaks = c(li[1], ls),
           counts = cumsum(fa),
           mids = (ls + li)/2,
           name = "x",
           equidist = TRUE)
class(ht) <- "histogram"


expr <- expression("Md"=LI[Md] +
                       frac(frac(n, 2) - F[frac(n, 2) - 1],
                            f[frac(n, 2)])*h)
plot(ht,
     freq = TRUE,
     axes = FALSE,
     xlab = "x",
     ylab = "Frequência acumulada",
     main = "")

labelsx <- parse(text = c('LI[Md]', '"Md"', 'LI[Md]+h'))
axis(1, c(70, 110), c("", ""))
axis(1, at = c(80, 90), labels = labelsx[c(1, 3)])
axis(1, at = 85, labels = labelsx[2], line = -0.8,
     col = "lightseagreen", col.axis = "lightseagreen")

labelsy <- parse(text = c('F[frac(n, 2)]', 'frac(n, 2)',
                          'F[frac(n, 2)-1]'))
axis(2, c(0, 100), c("", ""))
axis(2, at = c(30, 70), labels = labelsy[c(1, 3)], las = 1)
axis(2, at = 50, labels = labelsy[2], line = -0.3, las = 1,
     col = "lightseagreen", col.axis = "lightseagreen")

segments(80, 30, 90, 70, lty = 2)
segments(c(85, 80), c(-1.5, 50),
         c(85, 85), c(50, 50),
         col = "lightseagreen")

@

\end{frame}

