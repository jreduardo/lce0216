%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de dispersão - Desvio Padrão}
  \begin{block}{O {\bf desvio padrão}}
    corresponde à raiz quadrada da variância,
    $$S_X = \sqrt{S^2_X}$$
  \end{block}

  \begin{center}
    O desvio padrão tem a mesma unidade dos dados originais
  \end{center}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de dispersão - Desvio Padrão}
  {\bf Exemplo}: Considerando os dados de diâmetro das árvores:

  \begin{itemize}
  \item[]<2-> $$S_X = \sqrt{13,27} = 3,64 \mbox{ cm}$$
  \end{itemize}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de dispersão - Coeficiente de Variação}

  \begin{block}{O {\bf coeficiente de variação} é dado por}
    $$CV_X = 100 \frac{S_X}{\bar{x}}$$
    \small O CV é adimensional, pode-se comparar a dispersão de
    variáveis com diferentes unidades de medida.
  \end{block}

  \begin{block}{Orientação}
    \begin{center}
      \begin{table}
        \begin{tabular}{rcl}
          CV $\leq$ 10\% &$\Rightarrow$& baixo\\
          10\% $<$ CV $\leq$ 20\% &$\Rightarrow$ & médio\\
          20\% $<$ CV $\leq$ 30\% &$\Rightarrow$ & alto\\
          CV $>$ 30\% &$\Rightarrow$& muito alto\\
        \end{tabular}
      \end{table}
    \end{center}

    \small Não tome as sugestões como regra, o CV não é invariável a
    transformações.
  \end{block}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de dispersão - Coeficiente de Variação}
  {\bf Exemplo}: Considerando os dados de diâmetro das árvores:

  \begin{itemize}
  \item[]<2-> $$CV_X = 100\frac{3,64}{11,30} = 32,21\%$$
  \end{itemize}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de dispersão - Gráfico de Caixas}

  O gráfico de caixas ou \textbf{box-plot} resume a distribuição dos
  dados em uma representação bastante informativa. Têm-se como
  principais aspectos observados no box-plot:

  \begin{itemize}
    \item Simetria ou assimetria da distribuição;
    \item Amplitude de variação;
    \item Observações atípicas.
  \end{itemize}
  \vspace{0.5cm}\pause

  \begin{figure}[b]
    \includegraphics[width=9cm]{images/boxplot}
    \caption{Exemplo de um box-plot ou gráfico de caixas.}
  \end{figure}

\end{frame}

%-------------------------------------------
\begin{frame}[fragile]
  \begin{block}{{\bf Construção de um gráfico de caixas}}
    \begin{enumerate}
    \item Calcular o primeiro quartil ($Q_1$), a mediana (Md) e o
      terceiro quartil ($Q_3$);
    \item Calcular a AIQ = $Q_3 - Q_1$;
    \item Verificar a existência de observações atípicas, ou seja,
      valores menores do que $Q_1-1,5\mbox{AIQ}$ ou maiores do que
      $Q_3+1,5\mbox{AIQ}$;
    \item Calcular os limites inferior e superior dos dados sem
      considerar as observações atípicas;
    \item Construir o gráfico seguindo o esquema a seguir:
    \end{enumerate}
  \end{block}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Medidas de dispersão - Gráfico de Caixas}

  {\bf Exemplo}: Para os valores observados de produção anual de resina
  de árvores de {\it Pinus elliottii}, em kg, tem-se:

  \begin{center}
    \begin{tabular}{ccccc} \hline
      1,5 & 1,8 & 1,9 & 2,0 & 2,1 \\
      2,3 & 2,3 & 2,6 & 3,4 & 5,5 \\ \hline
    \end{tabular}
  \end{center}

<<echo=FALSE, fig.height=3, fig.width=7>>=

resina <- c(1.5, 1.8, 1.9, 2, 2.1, 2.3, 2.3, 2.6, 3.4, 5.5)
boxplot(resina, pch = 8, horizontal = TRUE)

@

\end{frame}

