\begin{frame}
  \frametitle{Quartis e Percentis}

  \begin{itemize}
  \item[]<1-> $P_{20} \; \Rightarrow \;$ classe: $10,0 \vdash 20,0$

  \item[]<2->
    $$\left\{
      \begin{array}{lll}
        20,0 - 10,0 & \longleftrightarrow & 0,39 \\
        P_{20} - 10,0 & \longleftrightarrow & 0,20 \\
      \end{array} \right.
    $$

    \begin{center}
      $P_{20} = 10 + \frac{20,0 - 10,0}{0,39}\times0,20$\\
      $P_{20} = 15,1 \mbox{cm}.$
    \end{center}
  \item[]
  \item[]<1-> $P_{75} \; \Rightarrow \;$ classe: $40,0 \vdash 50,0$

  \item[]<3->
    $$\left\{
      \begin{array}{lll}
        50,0 - 40,0 & \longleftrightarrow & 0,10 \\
        P_{75} - 40,0 & \longleftrightarrow & 0,04 \\
      \end{array} \right.
    $$

    \begin{center}
      $P_{75} = 40 + \frac{50,0 - 40,0}{0,10}\times0,04$\\
      $P_{20} = 44,0 \mbox{cm}.$
    \end{center}
  \end{itemize}
\end{frame}

