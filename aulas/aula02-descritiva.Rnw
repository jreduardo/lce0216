%-------------------------------------------
\begin{frame}
  \frametitle{Quartis e Percentis}

  \begin{itemize}
  \item {\bf Quartil}: generalização da mediana.\\[0.2cm]
    \begin{center}
      Quartil $\Rightarrow$ 4 partes
    \end{center}
    \begin{figure}[b]
      \includegraphics[width=7.5cm]{images/quartis} %
    \end{figure}
  \pause
  \item<2-> {\bf Percentil de ordem $100p$.}
    $$P_{100p} =
    \begin{dcases}
      \frac{x_{(np)} + x_{(np+1)}}{2}, &
          \text{se}\, np\, \text{for inteiro};\\
	x_{(\text{int}(np)+1)}, &
          \text{se}\, np\, \text{não for inteiro}.\\
    \end{dcases}
    $$
  \end{itemize}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Quartis e Percentis}

  {\bf Exemplo}: Para os valores observados de produção anual de resina
  de árvores de {\it Pinus elliottii}, tem-se:
  \vspace{0.3cm}

  \hspace*{1cm}
  \begin{minipage}[c]{1\textwidth}
    \begin{multicols}{2}
      \begin{center}
        \begin{tabular}{ccccc} \hline
          1,5 & 1,8 & 1,9 & 2,0 & 2,1 \\
          2,3 & 2,3 & 2,6 & 3,4 & 5,5 \\ \hline
        \end{tabular}
      \end{center}

      $n=10$\\
      Obter $P_{75} = Q_3$ e $P_{20}$
    \end{multicols}
  \end{minipage}
  \vspace{0.2cm}\pause

  \begin{itemize}
  \item <2->
    $P_{75} \quad \Rightarrow \quad np = 10 \times 0,75 = 7,5$
    \begin{align*}
      P_{75} = Q_3 &= x_{(\text{int}(7,5)+1)}\\
                   &= x_{(7+1)} = x_{(8)}\\
                   &=2,6\, \text{Kg}
    \end{align*}
    \vspace{-0.1cm}

  \item <3->
    $P_{20} \quad \Rightarrow \quad np = 10 \times 0,20 = 2$
    \begin{align*}
      P_{20} &= (x_{(2)} + x_{(3)})/2\\
             &= (1,8 + 1,9)/2\\
             &= 1,85\, \text{Kg}
    \end{align*}
  \end{itemize}
\end{frame}

