    \begin{figure}[b]
      \includegraphics[width=7cm]{images/cara_ou_coroa2.jpg} %
    \end{figure}
    $$\Omega = \{\mbox{(cara, cara), (cara, coroa),
      (coroa, cara), (coroa, coroa)}\}$$
  \end{itemize}
\end{frame}

