\begin{frame}
  \frametitle{Probabilidade condicional e independência}
  \begin{block}{Definição}
    Dados dois eventos quaisquer, $A$ e $B$, sendo $P(B) > 0$, definimos
    a probabilidade condicional de $A$ dado $B$, como sendo:
    $$P(A\mid B) = \frac{P(A\cap B)}{P(B)}$$
  \end{block}

  \begin{itemize}
  \item[]<2-> {\bf Observação:}
    $$P(B\mid A) = \frac{P(B\cap A)}{P(A)} = \frac{P(A\cap B)}{P(A)}$$
  \end{itemize}
\end{frame}

%-------------------------------------------
\begin{frame}
  \frametitle{Probabilidade condicional e independência}
  \begin{block}{Regra do Produto}
    $$P(A\cap B) = P(B) \times P(A\mid B)$$
    $$           = P(A) \times P(B\mid A)$$
  \end{block}
\end{frame}

